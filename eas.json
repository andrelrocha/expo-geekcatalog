{
  "cli": {
    "version": ">= 9.1.0"
  },
  "build": {
    "dev": {
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      },
      "ios": {
        "simulator": true
      }
    },
    "developmentClient": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview4": {
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "simulator": false
      }
    }
  }
}
